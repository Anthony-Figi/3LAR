package  {		import flash.display.MovieClip;			public class largeGrass2 extends MovieClip{						public function largeGrass2() {			this.x = randomRange(-200, 200);			this.y = 250;  			this.scaleX = 0.3; 			this.scaleY = 0.3;		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}			}	}