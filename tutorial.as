package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.utils.getTimer;	import flash.utils.Timer;	import flash.events.TimerEvent;	import com.caurina.transitions.Tweener;			public class tutorial extends MovieClip {				public var pulseTimer = new Timer(2500, 4);		public var live:Boolean = false;		public var lifecycle = 0;				public function tutorial() {			this.alpha = 0;			this.scaleX = 0.6;			this.scaleY = 0.6;			this.x = 50; 			this.y = 900;			pulseTimer.addEventListener(TimerEvent.TIMER, tween);			pulseTimer.start();			var frame = randomRange(1,7);			trace ("Frame", frame);			this.gotoAndStop(frame);					}				public function tween(event: Event) {			lifecycle += 1;			if(live == false){				Tweener.addTween(this, {							 alpha:0.6,							 time:2 							 });				live = true;			}			else if(live == true){				Tweener.addTween(this, {							 alpha:0,							 time:2,							 onComplete: destroy()							 });				live = false;			}		}				public function destroy () {			if(lifecycle >= 4){				this.parent.removeChild(this);			}		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}					}	}