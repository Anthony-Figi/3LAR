package  {		import flash.display.MovieClip;	import com.caurina.transitions.Tweener;			public class backgroundArt extends MovieClip {				public var DC;		public var Summer = new summer;		public var Autumn = new autumn;		public var Spring = new spring;		public var Winter = new winter;				public function backgroundArt(DC) {			this.DC = DC; 			this.addChild(Summer);		}				public function changeSeason(season) {			if(season == summer) {				this.addChild(Summer);				Tweener.addTween(Spring, {							 alpha:0,							 time:2							 });				Tweener.addTween(Summer, {							 alpha:1,							 time:2							 //onComplete: remover(Spring)							 });			}			else if(season == autumn) {				this.addChild(Autumn);				Tweener.addTween(Summer, {							 alpha:0,							 time:2							 });				Tweener.addTween(Autumn, {							 alpha:1,							 time:2							 //onComplete: remover(Summer)							 });			}			else if(season == winter) {				this.addChild(Winter);				Tweener.addTween(Autumn, {							 alpha:0,							 time:2							 });				Tweener.addTween(Winter, {							 alpha:1,							 time:2							 //onComplete: remover(Autumn)							 });			}			else if(season == spring) {				this.addChild(Spring);				Tweener.addTween(Winter, {							 alpha:0,							 time:2							 });				Tweener.addTween(Spring, {							 alpha:1,							 time:2							 //onComplete: remover(Winter)							 });			}		}				public function remover(season){			this.removeChild(season);		}				}	}