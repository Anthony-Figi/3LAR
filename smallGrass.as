package  {		import flash.display.MovieClip;			public class smallGrass extends MovieClip {						public function smallGrass() {			this.x = randomRange(-400, 400);			this.y = 250;  			this.scaleX = 0.5; 			this.scaleY = 0.5;		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}			}	}