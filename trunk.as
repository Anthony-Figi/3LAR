package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.net.*;	import flash.geom.ColorTransform;	import flash.utils.getDefinitionByName;	import flashx.textLayout.elements.GlobalSettings;			public class trunk extends MovieClip {				public var DC;				public function trunk(DC, xPos, yPos, scale) {			this.DC = DC;			this.y = yPos;			this.x = xPos;			this.alpha = 1;			this.scaleX = scale; 			this.scaleY = scale;			this.stop();						}				public function stopper (event: Event) {			if(this.currentFrame == 36) {				this.stop();			}		}			}}