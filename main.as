package  {		import flash.display.MovieClip;		public class main extends MovieClip {				public var tree;		public var leaves;				public function main() {									this.tree = new trunk(this);						var leafGenerator:Array = new Array();			for (var i:Number=0; i<500; i++) {				var xPos = randomRange(130, 900);				var yPos = randomRange(50, 500);				var zRot = randomRange(10, 170);    			var leaves:leaf = new leaf(xPos, yPos, zRot);    			leafGenerator.push(leaves);				};						this.addChild(tree);						for (var q:Number=0; q<leafGenerator.length;q++){				this.addChild(leafGenerator[q]); 			}				}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}					}	}