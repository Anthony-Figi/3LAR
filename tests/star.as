package  {		import flash.display.MovieClip;	import flash.utils.Timer;	import flash.events.TimerEvent; 			public class star extends MovieClip {				public var alphaUp:Boolean=false; 		public var alphaTimer:Timer = new Timer(10); 				public function star() {			this.x = randomRange(0, 1200);			this.y = randomRange(0, 700); 			var scale = randomRange(0, 100)/100;			this.scaleX = scale;			this.scaleY = scale;			this.alpha = randomRange(0, 50)/100;			var Bool = randomRange (0, 1); 			if (Bool == 1) {				alphaUp = true; 			}			else {				alphaUp = false; 			};			alphaTimer.addEventListener(TimerEvent.TIMER, changeAlpha); 			alphaTimer.start(); 					}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}				public function changeAlpha(e=null){			if (alphaUp == false && this.alpha > 0 && this.alpha < 0.6) {				this.alpha -= 0.01;				//this.scaleX -= 0.01;				//this.scaleY -= 0.01;			}			else if (alphaUp == true && this.alpha > 0 && this.alpha < 0.6) {				this.alpha += 0.01;				//this.scaleX += 0.01;				//this.scaleY += 0.01;			}			else if (this.alpha < 0.03) {				alphaUp = true; 				this.alpha = 0.05;				this.x = randomRange(0, 1200);				this.y = randomRange(0, 700); 			}			else if (this.alpha > 0.57) {				alphaUp = false; 				this.alpha = 0.55;			};		}					}	}