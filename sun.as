package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.utils.getTimer;	import flash.utils.Timer;	import flash.events.TimerEvent;		public class sun extends MovieClip {				public var DC; 		public var angle;		public var speed;		public var radius;		public var oldY:Number=0;		public var yDiff:Number;		public var changeTimer:Timer;		public var timeHappen:Boolean;				public function sun(DC) {						this.angle = 3;			this.speed = 0.05;			this.radius = 800;			this.DC = DC;			this.x = 0; 			this.y = 0;			this.addEventListener(Event.ENTER_FRAME, move);			this.timeHappen = false			// constructor code		}				public function move(e :Event) :void {			 yDiff = oldY - this.y;			 //trace(yDiff);			 oldY = this.y;			 var rad :Number = this.angle * (Math.PI / 180);			 this.x = 1850 - (1000 + this.radius * Math.cos(rad));			 this.y = 1300 - (500 + this.radius * Math.sin(rad));			 this.angle += this.speed;			 if (this.y < 300) {				 if (yDiff > 0 ){					 this.DC.frontNight.updateNight(-1);					 this.DC.backNight.updateNight(-1);				 }			 }			 if(this.y > 150) {				 				 if (yDiff < 0 ){					 this.DC.frontNight.updateNight(1);					 this.DC.backNight.updateNight(1);				 }				 			 };		}				public function changeTime(tempSpeed) {			this.timeHappen = true;			this.speed = tempSpeed;			changeTimer = new Timer(3000, 1);			changeTimer.addEventListener(TimerEvent.TIMER, restoreTime);			changeTimer.start();			}				public function restoreTime(event: Event) {			this.speed = 0.05;			trace("TIME RESTORED");			this.timeHappen = false;			changeTimer.removeEventListener(TimerEvent.TIMER, restoreTime);		}			 			 							}}