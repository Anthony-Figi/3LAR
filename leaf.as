 package  {		import flash.display.*;	import flash.text.*;	import flash.geom.ColorTransform;			public class leaf extends MovieClip {				public function leaf() {			var my_color:ColorTransform = new ColorTransform();			my_color.alphaMultiplier = 1;			//my_color.blueOffset = randomRange(-255, 255);			my_color.greenOffset = randomRange(-100, 0);			my_color.redOffset = randomRange(-100, 0);			this.transform.colorTransform = my_color;			 		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}		}}