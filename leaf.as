 package  {		import flash.display.*;	import flash.text.*;			public class leaf extends MovieClip {				public function leaf(tweet, xPos, yPos, zRot) {			 this.x = xPos;			 this.y = yPos;			 this.rotationZ = zRot;			 			 var calibri = new Calibri();			 			 var myFormat:TextFormat = new TextFormat();			 myFormat.size = 8;			 myFormat.align = TextFormatAlign.CENTER;			 myFormat.color = 0x000000;			 //myFormat.font = calibri.fontName;			 			 var myText:TextField = new TextField();			 myText.defaultTextFormat = myFormat;			 //myText.embedFonts = true;			 myText.antiAliasType = AntiAliasType.ADVANCED;			 myText.text = tweet;			 myText.wordWrap = true;			 myText.width = 90;			 myText.height = 65;			 myText.y = 0;			 myText.x = 0;			 			 this.addChild(myText);		}	}	}