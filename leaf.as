package  {		import flash.display.*;	import flash.text.*;			public class leaf extends MovieClip {				public function leaf(tweet, xPos,yPos, zRot) {			 this.x = xPos;			 this.y = yPos;			 this.rotationZ = zRot;			 			 var calibri = new Calibri();			 			 var myFormat:TextFormat = new TextFormat();			 myFormat.size = 5;			 myFormat.align = TextFormatAlign.CENTER;			 myFormat.font = calibri.fontName;			 			 var myText:TextField = new TextField();			 myText.defaultTextFormat = myFormat;			 myText.embedFonts = true;			 myText.antiAliasType = AntiAliasType.ADVANCED;			 myText.text = tweet;			 myText.wordWrap = true;			 myText.width = 60;			 myText.height = 35;			 myText.y = 5;			 myText.x = 10;			 			 this.addChild(myText);		}	}	}