package  {		import flash.display.MovieClip;			public class largeGrass3 extends MovieClip {						public function largeGrass3(xPos, yPos, scale) {			this.x = xPos; 				this.y = yPos;				this.scaleX = scale;				this.scaleY = scale;		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}			}	}