package  {		import flash.display.MovieClip;	import com.caurina.transitions.Tweener;	import flash.events.*;			public class bug extends MovieClip {						public function bug() {			this.x = randomRange(0, 1200);			this.y = randomRange(0, 1000);			this.addEventListener(Event.ENTER_FRAME, hover);		}				public function randomRange(minNum:Number, maxNum:Number):Number {			return (Math.floor(Math.random()*(maxNum - minNum +1)) + minNum);		}				public function hover (event: Event) {						trace(this.x, this.y);			var movement = randomRange(1, 4)			trace("RANDOM", movement);			if(movement == 1) {				Tweener.addTween(this, {				 x:+200,				 time:0.8,				 transition:"easeOutCubic"				 });			}			else if(movement == 2) {				Tweener.addTween(this, {				 x:-200,				 time:0.8,				 transition:"easeOutCubic"				 });			}			else if(movement == 3) {				Tweener.addTween(this, {				 y:+200,				 time:0.8,				 transition:"easeOutCubic"				 });			}			else if(movement == 4) {				Tweener.addTween(this, {				 y:-200,				 time:0.8,				 transition:"easeOutCubic"				 });			}		}			}	}